entity SmsCampaign {
	name String required,
	state Integer required,
	shortMsg String,
	startAt Instant,
	expiredAt Instant,
	pdu TextBlob,
	vasCloudPackage String,
	rateLimit Long,
}

entity CampaignSms {
  msisdn String,
  text String,
  state Integer,
  requestAt Instant,
}

relationship ManyToOne {
  CampaignSms{campaign(name)} to SmsCampaign
}

paginate all with pagination
dto all with mapstruct
